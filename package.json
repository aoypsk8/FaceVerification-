{
  "name": "face-verification-app",
  "version": "1.0.0",
  "description": "Face Verification application using Node.js, Express, and Amazon Rekognition",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node --watch src/app.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop face-verification",
    "pm2:restart": "pm2 restart face-verification",
    "docker:build": "docker build -t face-verification .",
    "docker:run": "docker-compose up -d"
  },
  "keywords": [
    "face-verification",
    "aws-rekognition",
    "express",
    "nodejs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.700.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "multer": "^1.4.5-lts.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  }
}
